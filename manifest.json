{
  "manifest_version": 2,
  "name": "Account Environment Label",
  "version": "0.6.0",
  "description": "Display AWS console environment label",
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "matches": [
      "https://*.console.aws.amazon.com/*"
    ],
    "js": [
      "content.js"
    ]
  }],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
